#import "HeartRate.h"

@implementation HeartRate

- (BOOL)isNormalByHeartRateValue {
    return self.heartRate >= 60 && self.heartRate <= 100;
}

- (BOOL)isNormalByBloodOxygenValue {
    return self.bloodOxygen >= 95 && self.bloodOxygen <= 100;
}

@end
